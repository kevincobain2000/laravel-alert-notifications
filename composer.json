{
    "name": "kevincobain2000/laravel-alert-notifications",
    "description": "Alert notifications of exceptions from your laravel application",
    "license": "MIT",
    "keywords": [
        "email",
        "exception",
        "laravel",
        "laravel-package",
        "exception-handling",
        "debugging"
    ],
    "authors": [
        {
            "name": "Pulkit Kathuria",
            "email": "kevincobain2000@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "~7.0|~8.0|~9.0|~10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.3.0"
    },
    "autoload": {
        "psr-4": {
            "Kevincobain2000\\LaravelAlertNotifications\\": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Kevincobain2000\\LaravelAlertNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kevincobain2000\\LaravelAlertNotifications\\AlertNotificationsServiceProvider"
            ]
        }
    }
}
