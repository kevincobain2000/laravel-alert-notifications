{
    "name": "kevincobain2000/laravel-alert-notifications",
    "description": "Alert notifications of exceptions from your laravel application",
    "license": "MIT",
    "version": "5.0",
    "keywords": ["email", "exception", "laravel", "laravel-5-package", "exception-handling", "debugging"],
    "authors": [
        {
            "name": "Pulkit Kathuria",
            "email": "kevincobain2000@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": ">=6.3",
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.7"
    },
    "autoload": {
        "psr-4": {
            "Kevincobain2000\\LaravelAlertNotifications\\": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "autoload-dev": {
        "psr-4": {
            "Kevincobain2000\\LaravelAlertNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kevincobain2000\\LaravelAlertNotifications\\AlertNotificationsServiceProvider"
            ]
        }
    }
}
